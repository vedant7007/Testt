//wap to concatinate a string at the end of another string without using built in fn//
#include<stdio.h>
#include<string.h>
void main()
{
char a[20],b[20];
printf("Enter the string1");
scanf("%s",&a);
printf("Enter the string2");
scanf("%s",&b);
int i,j;
i=strlen(a);
for(j=0;b[j]!='\0';i++,j++)
{
a[i]=b[j];
}
a[i]='\0';
puts(a);
}


//wap to copy the program of first string to another string without using strcpy progam//
#include<stdio.h>
void main()
{
char a[20],b[20];
printf("Enter the string1");
scanf("%s", a);
int i;
while (a[i] != '\0') {
b[i] = a[i];
i++;
}
 b[i] = '\0';
printf("The copied string is : %s\n",b);
}



