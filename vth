#include<stdio.h>
#include<string.h>
void main()
{
char str1[30],str2[20];
printf("enter the string");
gets(str1);
gets(str2);
strcpy(str2,str1);
printf("the copied of string is %s%s",str1,str2);
}



#include<stdio.h>
#include<string.h>
void main()
{
char a,str1[30],str2[40];
printf("enter the 1st string");
gets(str1);
printf("enter the 2nd string");
gets(str2);
a=(strcmp(str1,str2));
if(a==0)
{
printf("the  strings are same");
}
else
{
printf("the strings are not same");
}
}





#include<stdio.h>
#include<string.h>
void main()
{
char str1[30],str2[20];
printf("enter the 1st  string");
gets(str1);
printf("enter the 2nd string");
gets(str2);
strcat(str2,str1);
printf("the concatenated of string is %s",str2);
}
